{
    "git.showUntrackedFiles": true,   
    "git.untrackedChanges": "mixed",
    "python.defaultInterpreterPath": "${workspaceFolder}/venv/bin/python",
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/venv": true,
        "**/.devcontainer": true,
        "**/.history": true, 
        "**/.vscode": true,
        "**/.git": true,
        "**/misc": true
    }
}



